{
  "name": "@monorepo-frontend/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "husky",
    "lint": "eslint . && stylelint \"**/*.{css,scss}\" && markdownlint \"**/*.md\"",
    "lint:fix": "eslint . --fix && stylelint \"**/*.{css,scss}\" --fix && markdownlint \"**/*.md\" --fix",
    "format": "prettier --write \"**/*.{js,ts,vue,json,md,yml,yaml,css,scss}\"",
    "format:check": "prettier --check \"**/*.{js,ts,vue,json,md,yml,yaml,css,scss}\"",
    "type-check": "vue-tsc --noEmit",
    "lint:all": "nx run-many -t lint",
    "lint:affected": "nx affected -t lint",
    "test:all": "nx run-many -t test",
    "test:affected": "nx affected -t test",
    "generate:settings": "pnpm --filter @monorepo-frontend/styles generate:settings"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "markdownlint --fix",
      "prettier --write"
    ],
    "*.{json,yml,yaml}": [
      "prettier --write"
    ]
  },
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/pro-regular-svg-icons": "^7.1.0",
    "@fortawesome/vue-fontawesome": "^3.1.2",
    "@popperjs/core": "^2.11.8",
    "axios": "^1.12.2",
    "bootstrap": "^5.3.8",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3",
    "vue-toastification": "2.0.0-rc.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.37.0",
    "@nx/cypress": "21.6.4",
    "@nx/eslint": "21.6.4",
    "@nx/eslint-plugin": "21.6.4",
    "@nx/js": "21.6.4",
    "@nx/vite": "21.6.4",
    "@nx/vue": "21.6.4",
    "@nx/web": "21.6.4",
    "@nx/workspace": "21.6.4",
    "@swc-node/core": "~1.14.1",
    "@swc-node/register": "~1.11.1",
    "@swc/core": "~1.13.5",
    "@swc/helpers": "~0.5.17",
    "@types/node": "24.7.2",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "cypress": "^15.4.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-cypress": "^5.2.0",
    "eslint-plugin-vue": "^10.5.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jiti": "2.6.1",
    "jsdom": "~27.0.0",
    "lint-staged": "^16.2.4",
    "markdownlint-cli": "^0.45.0",
    "nx": "21.6.4",
    "prettier": "^3.6.2",
    "sass": "1.93.2",
    "stylelint": "^16.25.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-order": "^7.0.0",
    "tslib": "^2.8.1",
    "tsx": "^4.20.6",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.1",
    "vite": "^7.1.10",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.1"
  }
}
